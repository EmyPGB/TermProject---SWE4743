@page
@model TermProject.Pages.IndexModel
<center>
<h2>Welcome Back!</h2>
<h2>Please Select an Agent</h2>
</center>
<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Agent) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                <a asp-page="./CreateTrip">New Trip</a> 
                
            </td>
        </tr>
}
    </tbody>
</table>
